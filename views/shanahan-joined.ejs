<!DOCTYPE html>
<html lang="en">
  <head>
      <script type="text/javascript" src="../util.js"></script>
      <script type="text/javascript" src="../basement.js"></script>
      <script type="text/javascript" src="../floor1.js"></script>
      <script type="text/javascript" src="../floor2.js"></script>
      <script type="text/javascript" src="../floor3.js"></script>
      <script type="text/javascript" src="../shan-linked.js"></script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>Get a Room</title>

    <!-- Bootstrap core CSS -->
    <link href="../bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="../bootstrap/starter-template.css" rel="stylesheet">

  </head>

  <body>

    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Get a Room</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#about">About</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <div class="container">
      <!-- First row, first column, basement -->
      <h1 class = "page-header"> Shanahan Teaching and Learning Center </h1>
      <div class = "row">
        <div class = "col-md-6">
          <canvas id="canvas-basement" width="400" height="420" style="border:1px solid black"></canvas>
        </div><!-- /.col-md-6 -->
        <!-- First row, second column, first floor -->
        <div class = "col-md-6">
          <canvas id="canvas-floor1" width="400" height="420" style="border:1px solid black"></canvas>
        </div><!-- /.col-md-6 -->
      </div><!-- /.row -->

      <!-- Second row, first column, second floor -->
      <div class = "row">
        <div class = "col-md-6">
          <canvas id="canvas-floor2" width="400" height="420" style="border:1px solid black"></canvas>
        </div><!-- /.col-md-6 -->

        <!-- Secnd row, second column, third floor -->
        <div class = "col-md-6">
          <canvas id="canvas-floor3" width="400" height="420" style="border:1px solid black"></canvas>
        </div><!-- /.col-md-6 -->
      </div><!-- /.row -->

    </div><!-- /.container -->

    <script type="text/javascript">
          window.onload = function() {
              <% for ( room in rooms ) { %>
              fillRoom("<%= room %>", "<%= rooms[room] %>") 
              <% } %>
              placeImage("canvas-basement", "../shanahan-basement.gif");
              placeImage("canvas-floor1", "../shanahan-floor1.gif");
              placeImage("canvas-floor2", "../shanahan-floor2.gif");
              placeImage("canvas-floor3", "../shanahan-floor3.gif");
          }
    </script>
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="bootstrap/js/bootstrap.min.js"></script>
  </body>
</html>
